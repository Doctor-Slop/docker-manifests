server {
    listen 80;
    listen [::]:80;
    server_name linx.vtluug.org www.linx.vtluug.org;

    return 301 https://linx.li$request_uri;
}

server {
    listen 443 ssl;
    listen [::]:443 ssl;
    server_name linx.vtluug.org www.linx.vtluug.org;

    # ssl conf
    include /config/nginx/ssl.conf;

    return 301 https://linx.li$request_uri;
}
