server {
    listen 80;
    listen [::]:80;
    server_name element.vtluug.org;

    return 301 https://element.vtluug.org$request_uri;
}

server {
    listen 443 ssl;
    listen [::]:443 ssl;
    server_name element.vtluug.org;

    # ssl conf
    include /config/nginx/ssl.conf;

    client_max_body_size 100M;

    location / {
        # Proxy to container
        include /config/nginx/proxy.conf;
        proxy_pass http://element_element_1:80;
    }
}
